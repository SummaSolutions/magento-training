<?xml version="1.0"?>
<config>
    <modules>
        <Company_Feed>
            <version>1.0.0</version>
        </Company_Feed>
    </modules>
    <global>
        <models>
            <catalog>
                <rewrite>
                    <product>Company_Feed_Model_Catalog_Product</product>
                </rewrite>
            </catalog>
        </models>
        <helpers>
            <catalog>
                <rewrite>
                    <data>Company_Feed_Helper_Catalog_Data</data>
                </rewrite>
            </catalog>
        </helpers>
    </global>
    <frontend>
        <routers>
            <catalog>
                <args>
                    <modules>
                        <Company_Feed before="Mage_Catalog">Company_Feed_Catalog</Company_Feed>
                    </modules>
                </args>
            </catalog>
        </routers>
    </frontend>
</config>
